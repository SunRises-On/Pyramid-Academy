<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.HangmanFunctional.Controller.MenuController">
    <children>
        <Label fx:id="titleLabel" alignment="CENTER" layoutX="196.0" layoutY="48.0" prefHeight="66.0" prefWidth="207.0"
               text="Hangman" AnchorPane.bottomAnchor="285.6" AnchorPane.leftAnchor="196.0"
               AnchorPane.rightAnchor="196.8" AnchorPane.topAnchor="48.0">
            <font>
                <Font size="45.0"/>
            </font>
        </Label>
        <ImageView fx:id="hangmanImage" fitHeight="176.0" fitWidth="235.0" layoutX="182.0" layoutY="125.0"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Hangman%206.png"/>
            </image>
        </ImageView>
        <Button fx:id="startButton" layoutX="144.0" layoutY="323.0" mnemonicParsing="false" onAction="#login"
                text="Start"/>
        <Button fx:id="scoreButton" layoutX="377.0" layoutY="323.0" mnemonicParsing="false" text="High Score"/>
    </children>
</AnchorPane>
